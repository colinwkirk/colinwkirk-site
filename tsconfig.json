{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": false,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "pretty": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "noFallthroughCasesInSwitch": true,
    "outDir": "./dist",
    "rootDir": "./app"
  },
  "compileOnSave": true,
  "files": [
    "app/main.ts",
    "app/classes/resume/resume.activity.ts",
    "app/classes/resume/resume.activities.ts",
    "app/classes/resume/resume.education.ts",
    "app/classes/resume/resume.educations.ts",
    "app/classes/resume/resume.experience.ts",
    "app/classes/resume/resume.experiences.ts",
    "app/classes/resume/resume.skill.ts",
    "app/classes/resume/resume.skills.ts",
    "app/classes/modal-event.class.ts",
    "app/components/bottom-menu/bottom-menu.component.ts",
    "app/components/contact/contact.component.ts",
    "app/components/contact-form/contact-form.component.ts",
    "app/components/contact-modal/contact-modal.component.ts",
    "app/components/contact-modal/contact-modal.component.ts",
    "app/components/home/home.component.ts",
    "app/components/js-toys/js-toys.component.ts",
    "app/components/modal/modal.component.ts",
    "app/directives/modal-outlet/modal-outlet.component.ts",
    "app/components/not-found/not-found.component.ts",
    "app/components/resume/resume.component.ts",
    "app/components/top-menu/top-menu.component.ts",
    "app/services/modal/modal.service.ts",
    "app/services/rest/rest.service.ts",
    "app/services/contact-modal.service.ts",
    "app/app.component.ts",
    "app/app.routes.ts",
    "typings/index.d.ts"
  ]
}

